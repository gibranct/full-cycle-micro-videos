{
  "compilerOptions": {
    "incremental": true,
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "#category/*": ["./category/*"],
      "#seedwork/*": ["./@seedwork/*"]
    },
    "outDir": "./dist",
    "declaration": true
  },
  "include": ["src"]
}
